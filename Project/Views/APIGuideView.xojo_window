#tag WindowCode
	#tag Event
		Sub Open()
		  Self.ToolbarCaption = "API Guide"
		  Self.ToolbarIcon = IconGuide
		  Self.Viewer.LoadURL(BeaconAPI.URL)
		End Sub
	#tag EndEvent


#tag EndWindowCode

#tag Events Header
	#tag Event
		Sub Action(Item As BeaconToolbarItem)
		  Select Case Item.Name
		  Case "BackButton"
		    Viewer.GoBack
		  Case "ForwardButton"
		    Viewer.GoForward
		  End Select
		End Sub
	#tag EndEvent
	#tag Event
		Sub Open()
		  Me.LeftItems.Append(New BeaconToolbarItem("BackButton", IconToolbarBack, False))
		  Me.LeftItems.Append(New BeaconToolbarItem("ForwardButton", IconToolbarForward, False))
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events Viewer
	#tag Event
		Sub TitleChanged(newTitle as String)
		  Header.Caption = NewTitle
		End Sub
	#tag EndEvent
	#tag Event
		Sub DocumentComplete(URL as String)
		  Header.BackButton.Enabled = Me.CanGoBack
		  Header.ForwardButton.Enabled = Me.CanGoForward
		End Sub
	#tag EndEvent
#tag EndEvents
