#tag WindowCode
	#tag Method, Flags = &h0
		Shared Function Present(Parent As Window) As Beacon.LootSource.Packages
		  Dim Win As New ExportChooseEnvironmentDialog
		  Win.ShowModalWithin(Parent.TrueWindow)
		  If Win.mCancelled Then
		    Win.Close
		    Return CType(-1, Beacon.LootSource.Packages)
		  End If
		  
		  Dim Index As Integer = Win.EnvironmentMenu.ListIndex
		  Win.Close
		  
		  Select Case Index
		  Case 0
		    Return Beacon.LootSource.Packages.Island
		  Case 1
		    Return Beacon.LootSource.Packages.Scorched
		  End Select
		End Function
	#tag EndMethod


	#tag Property, Flags = &h21
		Private mCancelled As Boolean
	#tag EndProperty


#tag EndWindowCode

