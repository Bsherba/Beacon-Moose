		  Me.Add(ShelfItem.NewFlexibleSpacer)
		  Me.Add(IconPlayers, "Players", "players")
		  Me.Add(IconTames, "Creatures", "tames")
		  Me.Add(ShelfItem.NewFlexibleSpacer)
		  Me.SelectedIndex = 1
		End Sub
