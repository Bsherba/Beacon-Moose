#tag WindowCode
	#tag Event
		Sub Open()
		  Self.MinimumWidth = 750
		  Self.MinimumHeight = 368
		  
		  If Not Self.Document.HasConfigGroup(BeaconConfigs.ExperienceCurves.ConfigName) Then
		    Self.mConfig = New BeaconConfigs.ExperienceCurves
		    Self.Document.AddConfigGroup(Self.mConfig)
		  Else
		    Dim Config As Beacon.ConfigGroup = Self.Document.ConfigGroup(BeaconConfigs.ExperienceCurves.ConfigName)
		    Self.mConfig = BeaconConfigs.ExperienceCurves(Config)
		  End If
		  
		  Self.LevelsList.ColumnAlignment(0) = ListBox.AlignRight
		  Self.LevelsList.ColumnAlignment(1) = ListBox.AlignRight
		  
		  Self.CurveEditor.Curve = Self.mConfig.PlayerCurve
		  Self.FillPointFields(Self.mConfig.PlayerCurve)
		  Self.RefreshUI
		End Sub
	#tag EndEvent

	#tag Event
		Sub Resize(Initial As Boolean)
		  #Pragma Unused Initial
		  
		  Self.ViewSwitch.Left = (Self.Width - Self.ViewSwitch.Width) / 2
		End Sub
	#tag EndEvent


	#tag Method, Flags = &h21
		Private Function CurrentCurve() As Beacon.Curve
		  If Self.ViewingPlayerStats Then
		    Return Self.mConfig.PlayerCurve
		  Else
		    Return Self.mConfig.DinoCurve
		  End If
		End Function
	#tag EndMethod

	#tag Method, Flags = &h21
		Private Sub CurrentCurve(Assigns Curve As Beacon.Curve)
		  If Self.ViewingPlayerStats Then
		    Self.mConfig.PlayerCurve = Curve
		  Else
		    Self.mConfig.DinoCurve = Curve
		  End If
		End Sub
	#tag EndMethod

	#tag Method, Flags = &h21
		Private Sub FillLevelsList(Levels As Integer, TotalXP As Integer, Curve As Beacon.Curve)
		  Dim ScrollPosition As Integer = Self.LevelsList.ScrollPosition
		  Dim ListIndex As Integer = Self.LevelsList.ListIndex
		  
		  Self.LevelsList.DeleteAllRows()
		  
		  For I As Integer = 1 To Levels
		    Dim Time As Double = (I - 1) / (Levels - 1)
		    Dim XP As Integer = Round(Curve.Evaluate(Time, 0, TotalXP))
		    
		    Self.LevelsList.AddRow(Format(I, "0,"), Format(XP, "0,"))
		    If Self.LevelsList.LastIndex = ListIndex Then
		      Self.LevelsList.ListIndex = ListIndex
		    End If
		  Next
		  
		  Self.LevelsList.ScrollPosition = ScrollPosition
		End Sub
	#tag EndMethod

	#tag Method, Flags = &h21
		Private Sub FillPointFields(Curve As Beacon.Curve)
		  Self.Point1XField.Value = Round(Curve.Point(1).X * 100)
		  Self.Point1YField.Value = Round(Curve.Point(1).Y * 100)
		  Self.Point2XField.Value = Round(Curve.Point(2).X * 100)
		  Self.Point2YField.Value = Round(Curve.Point(2).Y * 100)
		End Sub
	#tag EndMethod

	#tag Method, Flags = &h21
		Private Function Point1XField() As RangeField
		  Return Self.PointFields(0)
		End Function
	#tag EndMethod

	#tag Method, Flags = &h21
		Private Function Point1YField() As RangeField
		  Return Self.PointFields(1)
		End Function
	#tag EndMethod

	#tag Method, Flags = &h21
		Private Function Point2XField() As RangeField
		  Return Self.PointFields(2)
		End Function
	#tag EndMethod

	#tag Method, Flags = &h21
		Private Function Point2YField() As RangeField
		  Return Self.PointFields(3)
		End Function
	#tag EndMethod

	#tag Method, Flags = &h21
		Private Sub RefreshUI(SkipField As RectControl = Nil)
		  Dim WasSettingUp As Boolean = Self.SettingUp
		  Self.SettingUp = True
		  
		  If Self.ViewingPlayerStats Then
		    If SkipField <> Self.MaxLevelField Then
		      Self.MaxLevelField.Text = Format(Self.mConfig.PlayerLevelCap, "0,")
		    End If
		    If SkipField <> Self.MaxExperienceField Then
		      Self.MaxExperienceField.Text = Format(Self.mConfig.PlayerMaxExperience, "0,")
		    End If
		    Self.SoftCapField.Text = Format(Self.mConfig.PlayerSoftLevelCap, "0,")
		    
		    Self.FillLevelsList(Self.mConfig.PlayerLevelCap, Self.mConfig.PlayerMaxExperience, Self.mConfig.PlayerCurve)
		    
		    Self.SoftCapField.Visible = True
		    Self.SoftCapLabel.Visible = True
		  Else
		    If SkipField <> Self.MaxLevelField Then
		      Self.MaxLevelField.Text = Format(Self.mConfig.DinoLevelCap, "0,")
		    End If
		    If SkipField <> Self.MaxExperienceField Then
		      Self.MaxExperienceField.Text = Format(Self.mConfig.DinoMaxExperience, "0,")
		    End If
		    
		    Self.FillLevelsList(Self.mConfig.DinoLevelCap, Self.mConfig.DinoMaxExperience, Self.mConfig.DinoCurve)
		    
		    Self.SoftCapField.Visible = False
		    Self.SoftCapLabel.Visible = False
		  End If
		  
		  Self.SettingUp = WasSettingUp
		End Sub
	#tag EndMethod

	#tag Method, Flags = &h21
		Private Function ViewingPlayerStats() As Boolean
		  Return Self.ViewSwitch.Items(0).Selected
		End Function
	#tag EndMethod


	#tag Property, Flags = &h21
		Private mConfig As BeaconConfigs.ExperienceCurves
	#tag EndProperty


#tag EndWindowCode

#tag Events CurveEditor
	#tag Event
		Sub Changed()
		  If Self.SettingUp Then
		    Return
		  End If
		  
		  Self.SettingUp = True
		  Self.CurrentCurve = Me.Curve
		  Self.FillPointFields(Me.Curve)
		  Self.RefreshUI
		  Self.ContentsChanged = True
		  Self.SettingUp = False
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events PointFields
	#tag Event
		Sub GetRange(index as Integer, ByRef MinValue As Integer, ByRef MaxValue As Integer)
		  MinValue = 0
		  MaxValue = 100
		End Sub
	#tag EndEvent
	#tag Event
		Sub TextChange(index as Integer)
		  If Self.SettingUp Then
		    Return
		  End If
		  
		  Dim Curve As New Beacon.Curve(Self.PointFields(0).Value / 100, Self.PointFields(1).Value / 100, Self.PointFields(2).Value / 100, Self.PointFields(3).Value / 100)
		  Self.CurveEditor.Curve = Curve
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events ViewSwitch
	#tag Event
		Sub Action(itemIndex as integer)
		  Dim Curve As Beacon.Curve = Self.CurrentCurve
		  If Curve = Nil Then
		    Return
		  End If
		  
		  Self.CurveEditor.Curve = Curve
		  Self.RefreshUI()
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events MaxLevelField
	#tag Event
		Sub TextChange()
		  If Self.SettingUp Then
		    Return
		  End If
		  
		  Dim Value As Integer = CDbl(Me.Text)
		  If Self.ViewingPlayerStats Then
		    Self.mConfig.PlayerLevelCap = Value
		  Else
		    Self.mConfig.DinoLevelCap = Value
		  End If
		  Self.ContentsChanged = True
		  Self.RefreshUI(Me)
		End Sub
	#tag EndEvent
	#tag Event
		Sub LostFocus()
		  Self.RefreshUI
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events MaxExperienceField
	#tag Event
		Sub TextChange()
		  If Self.SettingUp Then
		    Return
		  End If
		  
		  Dim Value As Integer = CDbl(Me.Text)
		  If Self.ViewingPlayerStats Then
		    Self.mConfig.PlayerMaxExperience = Value
		  Else
		    Self.mConfig.DinoMaxExperience = Value
		  End If
		  Self.ContentsChanged = True
		  Self.RefreshUI(Me)
		End Sub
	#tag EndEvent
	#tag Event
		Sub LostFocus()
		  Self.RefreshUI
		End Sub
	#tag EndEvent
#tag EndEvents
