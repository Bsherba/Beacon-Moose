#tag WindowCode
	#tag Event
		Sub RestoreToDefault()
		  Self.Document.MapCompatibility = Beacon.Maps.TheIsland.Mask
		End Sub
	#tag EndEvent

	#tag Event
		Sub SetupUI()
		  Self.Grid.Mask = Self.Document.MapCompatibility
		End Sub
	#tag EndEvent

	#tag Event
		Sub Shown(UserData As Variant = Nil)
		  #Pragma Unused UserData
		  
		  Self.SetupUI
		End Sub
	#tag EndEvent


	#tag Method, Flags = &h0
		Function ConfigLabel() As String
		  Return "Maps"
		End Function
	#tag EndMethod


#tag EndWindowCode

#tag Events Grid
	#tag Event
		Sub Changed()
		  If Self.SettingUp Then
		    Return
		  End If
		  
		  Self.SettingUp = True
		  Self.Document.MapCompatibility = Me.Mask
		  Self.Changed = True
		  Self.SettingUp = False
		End Sub
	#tag EndEvent
#tag EndEvents
