#tag WindowCode
	#tag Event
		Sub Open()
		  Self.TitleField.Text = Self.Document.Title
		  Self.DescriptionArea.Text = Self.Document.Description
		  Self.PublicFileCheckbox.Value = Self.Document.IsPublic
		  Self.PublicFileCheckbox.Visible = (Self.Controller.URL.Scheme = Beacon.DocumentURL.TypeCloud)
		End Sub
	#tag EndEvent


#tag EndWindowCode

#tag Events TitleField
	#tag Event
		Sub TextChange()
		  If Self.SettingUp Then
		    Return
		  End If
		  
		  Self.SettingUp = True
		  Self.Document.Title = Trim(Me.Text).ToText
		  Self.ContentsChanged = True
		  Self.SettingUp = False
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events DescriptionArea
	#tag Event
		Sub TextChange()
		  Self.PublicFileCheckbox.Enabled = Trim(Me.Text).Len > 32
		  
		  If Self.SettingUp Then
		    Return
		  End If
		  
		  Self.SettingUp = True
		  Self.Document.Description = Trim(Me.Text).ToText
		  Self.ContentsChanged = True
		  Self.SettingUp = False
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events PublicFileCheckbox
	#tag Event
		Sub Action()
		  If Self.SettingUp Then
		    Return
		  End If
		  
		  Self.SettingUp = True
		  Self.Document.IsPublic = Me.Value
		  Self.ContentsChanged = True
		  Self.SettingUp = False
		End Sub
	#tag EndEvent
#tag EndEvents
