#tag Events EditServerButton
	#tag Event
		Sub Action()
		  If Self.ProfilesList.SelCount <> 1 Then
		    Return
		  End If
		  
		  Dim Profile As Beacon.ServerProfile = Self.ProfilesList.RowTag(Self.ProfilesList.ListIndex)
		  If Not (Profile IsA Beacon.FTPServerProfile) Then
		    Return
		  End If
		  
		  Dim FTPProfile As Beacon.FTPServerProfile = Beacon.FTPServerProfile(Profile)
		  Dim Edited As Beacon.FTPServerProfile = FTPProfileDialog.Present(FTPProfile)
		  If Edited = Nil Then
		    Return
		  End If
		  
		  Self.ProfilesList.RowTag(Self.ProfilesList.ListIndex) = Edited
		  Self.ProfilesList.Cell(Self.ProfilesList.ListIndex, 0) = Edited.Name
		  Self.ProfilesList.Cell(Self.ProfilesList.ListIndex, 2) = Edited.Host + ":" + Edited.Port.ToText
		  Self.ProfilesList.Sort
		End Sub
	#tag EndEvent
