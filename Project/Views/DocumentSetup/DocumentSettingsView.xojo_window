#tag Events RemoveServerButton
	#tag Event
		Sub Action()
		  If Self.ProfilesList.SelCount = 0 Then
		    Return
		  End If
		  
		  For I As Integer = Self.ProfilesList.ListCount - 1 DownTo 0
		    If Self.ProfilesList.Selected(I) Then
		      Self.ProfilesList.RemoveRow(I)
		    End If
		  Next
		End Sub
	#tag EndEvent
